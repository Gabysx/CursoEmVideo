algoritmo "Soma"
// Função :
// Autor :
// Data : 04/02/2022
// Seção de Declarações 
var
   num, num1, soma: inteiro

inicio
      Escreva("Digite um numero: ")
      leia(num)
      Escreval("Digite mais um numero")
      leia(num1)
      soma <- num + num1
      Escreval("A soma entre esses numeros é : ", soma)
fimalgoritmo
