algoritmo "FazSomaProcedimeto"
// Função :
// Autor :
// Data : 09/02/2022
// Seção de Declarações 
var
    X, Y : inteiro
    Procedimento Soma(A,B: inteiro)
    inicio
          Escreval("Recebi o valor ", A)
          Escreval("Recebi o valor ", B)
          Escreval("A soma entre os dois e ", A + B )
    
    FimProcedimento
inicio

    X<- 5
    Y <- 3
    Soma(X,Y)
fimalgoritmo
