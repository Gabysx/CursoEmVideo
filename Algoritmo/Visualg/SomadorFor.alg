algoritmo "somadorFor"
// Função :
// Autor :
// Data : 07/02/2022
// Seção de Declarações
var
    num, cont, soma: inteiro
inicio
      soma <- 0
     para cont <- 1 ate 5 faca
        Escreval("Digite um valor: ")
        leia(num)
        soma <- soma + num
     FimPara
      Escreval("a soma dos valores é, ", soma)
fimalgoritmo
