algoritmo "senha"
// Função :
// Autor :
// Data : 20/02/2022
// Seção de Declarações 
var
 s : inteiro
inicio
 s <- 0
      enquanto (s <> 2002) faca
            Escreval("Digite a senha ")
            leia(s)
               Escreval("Senha Inválida")
      fimenquanto
      limpatela
      Escreval("Acesso Permitido")
fimalgoritmo
