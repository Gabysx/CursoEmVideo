algoritmo "raio"
// Função :
// Autor :
// Data : 20/02/2022
// Seção de Declarações 
var
    raio, calc:real

inicio
         pi <- 3.14159
         Escreval("Digite qual é o raio do cículo")
         leia(raio)
         calc <- pi * (raio ^ 2)
         
         Escreval(" Area completa do círculo = ", calc:6:4)
         
fimalgoritmo
