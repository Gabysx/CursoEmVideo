algoritmo "TesteVetores"
// Função :
// Autor :
// Data : 11/02/2022
// Seção de Declarações 
var
  v: vetor[1..6] de inteiro
  c: inteiro
inicio

      Para c <- 1 ate 6 faca
         Escreva("Digite o ", c, "º valor: ")
         leia(v[c])
      FimPara
      
      Para c <- 1 ate 6 faca
         Escreva("[", v[c], "]")
      FimPara
fimalgoritmo
