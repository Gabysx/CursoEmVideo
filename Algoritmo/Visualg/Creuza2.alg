algoritmo "conversorWhile"
// Função :
// Autor :
// Data : 05/02/2022
// Seção de Declarações 
var
     reais, conversao: real

inicio
            Escreval("Qual é o seu valor em reais?")
            leia(reais)
            conversao <- reais/5.33
            Escreval(" Seu valor em dólar ", conversao:5:1, "US$")
fimalgoritmo
